{
    "name": "gogain/laravel-phone-verification",
    "description": "SMS phone verification component",
    "version": "1.0.1",
    "license": "MIT",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Nikita Tsurkan",
            "email": "nikita.tsurkan@goga.zone"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/console": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.22",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
          "Curkan\\LaravelPhoneVerification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Curkan\\LaravelPhoneVerification\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Curkan\\LaravelPhoneVerification\\Providers\\EloquentFilterServiceProvider"
        ]
      }
    }
}
